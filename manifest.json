{
  "manifest_version": 3,
  "name": "Youtube Popup Remover",
  "version": "1.0",
  "description": "Removes the annoying Youtube popup if you have adblocked enabled",
  "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": ["https://developer.chrome.com/docs/extensions/*", "https://youtube.com/*", "https://www.youtube.com/*"]
    }
  ]
}
